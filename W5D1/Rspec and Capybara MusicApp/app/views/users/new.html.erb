<h1>Create a New user </h1>
<br>
<%= render "shared/errors" %>
<form action= '/users' method="post">
  <input type="hidden" name="authenticity_token"
    value=" <%= form_authenticity_token %>">

  <label>Email
  <input id = "Email" type="text" name="user[email]" value=""></label>
  <br>
  <label>Password
  <input id = "Password" type="password" name="user[password]" value=""></label>
  <br>
  <input type="submit" value="Sign up!">

  <p> <%= link_to "Return to homepage", bands_url, class: "return_link" %></p>

</form>
